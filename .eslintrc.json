{
  "env": { 
    "browser": true,
    "node": true,
    "es2021": true 
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "next/core-web-vitals"
  ],
  "plugins": [
    "react", 
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
      "react/prop-types": 0 ,
      "react/display-name": "off"
  }
}
